        yy = ( (alarm[2]) / room_trans) * 640 - 640
        yyy = ( (alarm[2]) / room_trans) * 640
        var px = x
        var py = yyy + (room_trans - alarm[2]) / room_trans * 64 - 64
    }
     if surface_exists(new_room_surface)
